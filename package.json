{
  "name": "nova-boop",
  "version": "1.0.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "format": "prettier -w \"{src,tests}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
    "test:unit": "vitest run --coverage",
    "start:dev": "nodemon -r dotenv/config --inspect=9999 src/index.ts",
    "version:patch": "version patch",
    "version:minor": "version minor",
    "version:major": "version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fethca/nova-boop.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fethca/nova-boop/issues"
  },
  "homepage": "https://github.com/fethca/nova-boop#readme",
  "dependencies": {
    "@fethcat/logger": "^1.0.0",
    "@fethcat/shared": "^0.0.3",
    "@fethcat/store": "^0.0.1",
    "@fethcat/validator": "^0.0.3",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "fuse.js": "^7.0.0",
    "htmlparser2": "^9.1.0",
    "ioredis": "5.3.2",
    "latinize": "^2.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "moment": "^2.30.1",
    "puppeteer": "^22.4.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-block-resources": "^2.4.3",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "random-useragent": "^0.5.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@fethcat/eslint-plugin": "^0.0.3",
    "@fethcat/prettier-config": "^1.0.2",
    "@fethcat/tests": "^0.0.10",
    "@fethcat/version": "^0.0.8",
    "@types/latinize": "^0.2.18",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.uniqwith": "^4.5.9",
    "@types/node": "^20.3.0",
    "@types/random-useragent": "^0.3.3",
    "@types/spotify-web-api-node": "^5.0.11",
    "mockdate": "^3.0.5",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "prettier": "@fethcat/prettier-config",
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "execMap": {
      "ts": "tsx"
    },
    "watch": [
      "src/"
    ],
    "ext": "js,json,ts"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@fethcat/back"
    ]
  }
}
